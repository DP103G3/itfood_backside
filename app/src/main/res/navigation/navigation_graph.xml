        <action
            android:id="@+id/action_shopManagementFragment_to_shopDetailFragment"
            app:destination="@id/shopDetailFragment" />
        <action
            android:id="@+id/action_shopManagementFragment_to_shopDishListFragment"
            app:destination="@id/shopDishListFragment" />
    </fragment>
    <fragment
